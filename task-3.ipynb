    "# Write a code that checks if two given strings are anagrams\n",
    "# Sample Input: Mary Army\n",
    "# Output: Yes\n",
    "\n",
    "import re\n",
    "s = 'Mary Army'\n",
    "s1,s2 = re.split(' ',s)\n",
    "if(sorted(s1.lower())==sorted(s2.lower())):\n",
    "      print(\"yes\")\n",
    "else:\n",
    "    print(\"no\")"
   ]
  }
